[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Mariya Harizanova
	email = mharizanova@yahoo.com
	namemharizanova8703git = config
[init]
	defaultBranch = Note
[core]
	excludesfile = /Users/MariyaHarizanova/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree 2.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/MariyaHarizanova/.stCommitMsg
[pull]
	rebase = false
